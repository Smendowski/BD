DROP TABLE "subcategories";
CREATE TABLE "subcategories" (
	"sid" int4 PRIMARY KEY,
	"name" character varying(30),
	"cid" int4,
	"sserid" int4,
	FOREIGN KEY (cid) REFERENCES categories(cid) ON UPDATE CASCADE ON DELETE CASCADE,
		UNIQUE(cid,name),
		UNIQUE(cid,sid)
);
